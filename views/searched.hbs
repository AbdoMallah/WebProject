<section class = "searchBodySection">
    {{#if SearchError}}
    <ul>
        {{#each SearchError}}
        <li class="errorMSG"> {{this}} </li>
        {{/each}}
    </ul>
    {{/if}}
    {{#if amountPosts}}
        <div>
            <h5>Searched For: {{clearedSeachTerm}}</h5>
            <h6> Results: {{amountPosts}}</h6>
        </div>
    {{/if}}
    <div class = "posts">
    {{#each searchedPost}}
        <a href="/post/{{Id}}" class="postLink">
        <section class = "post"> 
            <img src="{{Image}}" class="img-responsive" id="postImage" alt="Post Image"> </img>
            <div id = "postInfo">
                <h3>{{Title}}</h3>
                
                <p>{{Prise}} Kr</p>
                <p id = "postParagraph">{{this.Description}}</p>
            </div>
        </section>
        </a> 
    {{/each}}  
    </div>
</section>