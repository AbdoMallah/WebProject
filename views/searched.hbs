<section class = "bodySection">
     {{#if SearchError}}
    <ul>
        {{#each SearchError}}
        <p class="errorMSG"> {{this}} </p>
        {{/each}}
    </ul>
    {{/if}}
    <div class = "posts">
    {{#each searchedPost}}
        <a href="/post/{{Id}}" class="postLink">
        <section class = "post"> 
            <img src="{{Image}}" class="img-responsive" id="postImage" alt="Post Image"> </img>
            <div id = "postInfo">
                <h3>{{Title}}</h3>
                
                <p>{{Prise}} Kr</p>
                <p id = "postParagraph">{{this.Description}}</p>
            </div>
        </section>
        </a> 
    {{/each}}  
    </div>
</section>